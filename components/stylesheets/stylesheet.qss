* 
{
    font-family : Abel;
    font-size : 15px;
}

QMainWindow, QStatusBar
{
    color: white;
    background-color : #4b546e;
}

QPushButton
{
    color: white;
    background-color : #4e6ecc;
    border : 0px;
}

QPushButton:hover
{
    background-color : #6485e8;
    border : 0px;
}

QPushButton:disabled
{
    color: #9497a1;
    background-color : gray;
    opacity : 50;
}

QLabel
{
    color : white;
    font-family : Abel;
}

QLabel#app_name
{
    color: white;
    font-family : Abel;
    font-size : 25px;
}

QLabel#label_logRating, QLabel#label_logRating_1, QLabel#label_logRating_2
{
    font-weight : bold;
    font-size : 35px;
}

QLabel#app_logo_pix
{
    background-color : #202430;
}

QFrame#frame_navigation, QMenuBar
{
    color: white;
    background-color : #202430;
}

QFrame#frame_navigation *
{
    color: white;
    background-color : #202430;
}

QFrame#frame_navigation .QPushButton:hover
{
    background-color : #33394a;
}

QFrame#submenu_myDay, QFrame#submenu_graphing, QFrame#submenu_settings, QFrame#submenu_help, QFrame#submenu_resources
{
    background-color : #252b3b;
}

QFrame#submenu_myDay *, QFrame#submenu_graphing *, QFrame#submenu_settings *, QFrame#submenu_help *, QFrame#submenu_resources *
{
    color: white;
    background-color : #252b3b;
}

QFrame#submenu_myDay *:hover, QFrame#submenu_graphing *:hover, QFrame#submenu_settings *:hover, QFrame#submenu_help *:hover, QFrame#submenu_resources *:hover
{
    background-color : #2f364a;
}

QMessageBox
{
    color : white;
    background-color : #141721;
}

QMessageBox *
{
    color : white;
    background-color : #141721;
}

QMessageBox .QPushButton
{
    color : black;
    background-color : #b5c0e6;
    padding : 5px;
}

QMessageBox .QPushButton:hover
{
    background-color : #dce2f5;
}

QErrorMessage
{
    color : white;
    background-color : #141721;
}

QErrorMessage *
{
    color : white;
    background-color : #141721;
}

QErrorMessage .QPushButton
{
    color : black;
    background-color : #b5c0e6;
    padding : 5px;
}

QErrorMessage .QPushButton:hover
{
    background-color : #dce2f5;
}

QPushButton:disabled
{
    color: #9497a1;
    background-color : gray;
    opacity : 50;
}